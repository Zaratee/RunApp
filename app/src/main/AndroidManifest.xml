<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zarate.runapp">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".InicioApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".RegistroApp" />
        <activity android:name=".MenuApp" />
        <activity android:name=".AlimentacionApp" />
        <activity
            android:name=".EjercicioActi1"
            android:label="@string/title_activity_ejercicio_acti1"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>